

#include <stdio.h>
#include <stdlib.h>

#define MAX 100001
char getLowest(int lC,int sC,int gC,int i);
char L[MAX], G[MAX], S[MAX];


int main() { 
	//FILE* inf = fopen("chimin.txt","r");
	//	//FILE* outf = fopen("chimout.txt","w");
	int n, i;
	scanf("%d", &n);
	scanf("%s %s %s",L, S, G);
	int lC, sC, gC;	
	for (i = 0; i < n; i++) {
		char c = getLowest(lC,sC,gC,i);
		printf("%c\n%c\n%c\n",L[i],S[i],G[i]);
		if( L[i] == c) lC++;
		if( S[i] == c) sC++;
		if( G[i] == c) gC++;
		printf("L: %d\nS: %d\nG: %d\n\n",lC, sC, gC);
	}

	printf("L: %d\nS: %d\nG: %d\n",lC, sC, gC);
	return 0;
}

char getLowest(int lC,int sC,int gC,int i) {
	char p;
	int min = 0;
	if (lC <= min) {
		min = sC;
		p = L[i];
	}
	if (sC <= min) {
		min = sC;
		p = S[i];
	}
	if (gC <= min) {
		min = gC;
		p = G[i];
	}
	printf("Lowest is %d\n",min);
	return p;
}

