
#include <stdio.h>
#include <stdlib.h>

#define MAX 10010

typedef struct node *Node;

struct node{
    int v;
    Node l;
};

typedef struct _list{
    Node end;
    int size;
}*List;


void append(List a, Node n) {
    if (a->end != NULL) {
        (a->end)->l = n;
    }
    a->end = n;
    a->size++;
    return;
}

List a[MAX];
Node b[MAX];

