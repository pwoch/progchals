#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
	int s = 5;
	int i;
	char a[100];
	while (scanf("%s",a)) {
		int l = strlen(a);
		for ( i = 0; i < l; i++) {
			switch(a[i]) {
				case('U') :
					s += ((a[i]>3) ? -3 : 0);
					break;
				case('R') :
					s += ((a[i]%3) ? 1 : 0);
					break;
				case('D') :
					s += ((a[i]<7) ? 3 : 0);
					break;
				case('L') :
					s += (((a[i]-1)%3) ? -1 : 0);
					break;
			}
		}
		printf("%s",s);
	}
	printf("\n");
	return 0;
}

